<div class="card-footer text-body-secondary">
    <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-end" id="navBar">
            <li class="page-item {{#if results.hasPrevPage}} {{else}}disabled{{/if}}">
                <a class="page-link" {{#if results.hasPrevPage}} href="/products{{results.prevLink}}" {{/if}} >Anterior</a>
            </li>
            {{#each results.navBar}}
            <li class="page-item"><a class="page-link {{this.status}}" href="/products/?pagina={{this.page}}">{{this.page}}</a></li>
            {{/each}}
            <li class="page-item {{#if results.hasNextPage}} {{else}}disabled{{/if}}">
                <a class="page-link" {{#if results.hasNextPage}} href="/products{{results.nextLink}}" {{/if}}>Siguiente</a>
            </li>
        </ul>
    </nav>
</div>